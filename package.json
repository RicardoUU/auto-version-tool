{
  "name": "auto-version-tool",
  "version": "1.0.0",
  "description": "根据git commit历史自动修改版本号的前端脚手架工具",
  "main": "dist/index.js",
  "bin": {
    "auto-version": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "prepare": "npm run build"
  },
  "keywords": [
    "version",
    "git",
    "changelog",
    "semantic-version",
    "frontend",
    "cli"
  ],
  "author": "Ricardo",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0",
    "conventional-commits-parser": "^5.0.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/inquirer": "^8.2.10",
    "@types/semver": "^7.5.4",
    "@types/mustache": "^4.2.5",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
