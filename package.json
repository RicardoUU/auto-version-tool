{
  "name": "auto-version-tool",
  "version": "1.4.0",
  "description": "根据git commit历史自动修改版本号并生成changelog的CLI工具 (Automatically bump version & generate changelog based on git commits)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "auto-version": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "auto-version.config.js",
    "templates"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [
    "version",
    "git",
    "changelog",
    "semantic-version",
    "frontend",
    "cli",
    "conventional-commits",
    "semantic-release"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RicardoUU/auto-version-tool.git"
  },
  "bugs": {
    "url": "https://github.com/RicardoUU/auto-version-tool/issues"
  },
  "homepage": "https://github.com/RicardoUU/auto-version-tool#readme",
  "author": "Ricardo",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0",
    "conventional-commits-parser": "^5.0.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/inquirer": "^8.2.10",
    "@types/semver": "^7.5.4",
    "@types/mustache": "^4.2.5",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
